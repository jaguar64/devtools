MACFLAGS = -fb -g
STADDR = 4000
ALNFLAGS = -v -v -e -g -l -s -a $(STADDR) 10000 20000
#====================================================================
#	Default Rules
#====================================================================
.SUFFIXES:	.o .s

.s.o:
	mac $(MACFLAGS) $*
#====================================================================
OBJ   = demo.o

demo.abs: $(OBJ)
	aln $(ALNFLAGS) $(OBJ)

